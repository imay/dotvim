call pathogen#infect()
syn on
set ai
set cindent
set showmatch
set expandtab
set shiftwidth=4
set tabstop=4
set nu
set ignorecase
set fileencodings=utf-8,gbk
set hls
set helplang=cn
set incsearch

filetype plugin indent on

noremap <C-J> <C-W>j
noremap <C-K> <C-W>k
noremap <C-H> <C-W>h
noremap <C-L> <C-W>l
